###################################################################
record(ao, "$(P)$(R)NormalData0")
{
    field(DESC, "Get the latest ADC value") 
    field(DTYP, "asynFloat64")
    field(OUT,  "@asyn($(PORT),$(ADDR))NORMAL_DATA_0")
}
record(ai, "$(P)$(R)NormalData0_RBV")
{
    field(DESC, "Get the latest ADC value") 
    field(PINI, "1")
    field(DTYP, "asynFloat64")
    field(EGU, "g")
    field(PREC, "3")
    field(INP,  "@asyn($(PORT),$(ADDR))NORMAL_DATA_0")
    field(SCAN, "I/O Intr")
#    field(SCAN, "1 second")
}

