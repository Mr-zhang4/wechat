  field(EGU , "lbf")
  field(CALC, "A*B")
  field(PREC, "4")
  field(SCAN, ".1 second")
}

record(mbbo,"$(P)$(R)Direction0")
{
   field(ZRST, "Compression")
   field(ONST, "Tension")
   field(ZRVL, "0")
   field(ONVL, "1")
   field(FLNK, "$(P)$(R)CalcPressure0")
}

